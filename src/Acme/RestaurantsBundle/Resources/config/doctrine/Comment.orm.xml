<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Acme\RestaurantsBundle\Entity\Comment" table="comment">
    <id name="commentId" type="integer" column="comment_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="id" type="integer" column="id" nullable="false"/>
    <field name="placeId" type="text" column="place_id" nullable="false"/>
    <field name="commentUser" type="text" column="comment_user" nullable="false"/>
    <field name="commentApproved" type="integer" column="comment_approved" nullable="false"/>
    <field name="commentCreated" type="datetime" column="comment_created" nullable="false"/>
    <field name="commentUpdated" type="datetime" column="comment_updated" nullable="false"/>
    <field name="commentContent" type="text" column="comment_content" nullable="false"/>
   

    
    
  
    
    
  </entity>
</doctrine-mapping>
