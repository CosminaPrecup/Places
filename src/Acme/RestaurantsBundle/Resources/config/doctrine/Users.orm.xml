<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Acme\RestaurantsBundle\Entity\Users" table="users">
    <id name="usersId" type="integer" column="users_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="usersName" type="string" column="users_name" length="25" nullable="false"/>
    <field name="usersFirstname" type="string" column="users_firstname" length="25" nullable="false"/>
    <field name="usersPassword" type="string" column="users_password" length="25" nullable="false"/>
  </entity>
</doctrine-mapping>
